<!--Studio Template -->
<csp:StudioInteractiveTemplate name="HTML Script" type="CSP,CLS"  description="Inserts an HTML SCRIPT tag." id="HTMLScript">
<csp:class DOMAIN="%TemplateMessages">
<html>
<head>
<style type="text/css">
<!--
-->
</style>
</head>
<body>
<script language="cache" runat="server">
	Do ##class(%template.htmlcolor).DrawTitle($$$Text("HTML Script Wizard"))
</script>
<!-- YSD2449<h1>#($$$Text("HTML Script Wizard"))#</h1>-->
<div class="Description">#($$$Text("Enter type and content of the script tag and then press OK."))#</div>
<hr>
<form name="form" action="HTMLScript2.csp">
<table cellspacing="5" cellpadding="0">
<tr><td class="Label">#($$$Text("Language"))#:</td><td>
<select name="language">
<option value="Cache" selected>Cache
<option value="Basic">Basic
<option value="MVBasic">MVBasic
<option value="JavaScript">JavaScript
</select>
</td></tr>
<tr><td class="Label">#($$$Text("Script"))#:</td><td><textarea name="script" rows="10" cols="40">#($get(%request.Data("SelectedText",1)))#</textarea></td></tr>
</table>
<hr>
<div align="right"><input type="submit" value=" OK " name="submit" textid=""></div>
</form>
</body>
</html>
