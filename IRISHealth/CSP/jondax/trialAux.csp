<script type="text/javascript">
    // Function to get the query parameter by name
    function getQueryParam(param) {
        const urlParams = new URLSearchParams(window.location.search);
        return urlParams.get(param);
    }

    // Get the 'section' query parameter
    const section = getQueryParam('section');

    // Set the iframe src based on the section
    const iframe = document.getElementById('content-frame');
    switch (section) {
        case 'terms':
            iframe.src = 'trialTermsofUse.csp'; // Load Terms of Use content
            break;
        case 'privacy':
            iframe.src = 'trialPrivacyPolicy.csp'; // Load Privacy Policy content
            break;
        case 'contact':
            iframe.src = 'contact.csp'; // Load Contact Us content
            break;
        default:
            iframe.src = 'trialPrivacyPolicy.csp'; // Load some default content
    }
</script>
