;-------------------------------------------------------------------------
; NDISISC.INF -- InterSystems' NDIS Protocol Driver
;
; Copyright (c) 2011, InterSystems Corporation
;-------------------------------------------------------------------------
[version]
Signature   = "$Windows NT$"
Class       = NetTrans
ClassGUID   = {4d36e975-e325-11ce-bfc1-08002be10318}
Provider    = %Msft%
DriverVer   =08/27/2012,6.1.7600.16385
CatalogFile= ndisiscamd64.cat

[Manufacturer]
%Msft%=MSFT,NTx86,NTia64,NTamd64

[MSFT.NTx86]
%NDISISC_Desc%=Install, MS_NDISPROT

[MSFT.NTia64]
%NDISISC_Desc%=Install, MS_NDISPROT

[MSFT.NTamd64]
%NDISISC_Desc%=Install, MS_NDISPROT

;-------------------------------------------------------------------------
; Installation Section
;-------------------------------------------------------------------------
[Install]
AddReg=Inst_Ndi
Characteristics=0x0 ; 
CopyFiles=CpyFiles_Sys

;-------------------------------------------------------------------------
; Ndi installation support
;-------------------------------------------------------------------------
[Inst_Ndi]
HKR,Ndi,Service,,"Ndisisc"
HKR,Ndi,HelpText,,%NDISISC_HelpText%
HKR,Ndi\Interfaces, UpperRange,, noupper
HKR,"Ndi\Interfaces","LowerRange",,"ndis5,ndis4,ndis5_prot"

;-------------------------------------------------------------------------
; Service installation support
;-------------------------------------------------------------------------
[Install.Services]
AddService=Ndisisc,,NDISISC_Service_Inst

[NDISISC_Service_Inst]
DisplayName     = %NDISISC_Desc%
ServiceType     = 1 ;SERVICE_KERNEL_DRIVER
StartType       = 1 ;SERVICE_AUTO_START
ErrorControl    = 1 ;SERVICE_ERROR_NORMAL
ServiceBinary   = %12%\ndisisc.sys
LoadOrderGroup  = NDIS
Description     = %NDISISC_Desc%

[Install.Remove.Services]
DelService=Ndisisc,0x200

;-------------------------------------------------------------------------
; Declare Destination Directories for file copy/deletion
;-------------------------------------------------------------------------
[SourceDisksNames]
1=%DiskDescription%,"",,

[SourceDisksFiles]
ndisisc.sys=1


[DestinationDirs]
CpyFiles_Sys    = 12    ; DIRID_DRIVERS


;-------------------------------------------------------------------------
; Files to Copy/Delete - Referenced by Install and Remove sections above
;-------------------------------------------------------------------------
[CpyFiles_Sys]
Ndisisc.sys,,,2

[Strings]
Msft = "InterSystems"
DiskDescription = "InterSystems Protocol Driver Disk"
NDISISC_Desc = "InterSystems NDIS Protocol Driver"
NDISISC_HelpText = "A driver to support user-mode I/O on NDIS devices"






